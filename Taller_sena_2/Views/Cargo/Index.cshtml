@model IEnumerable<Taller_sena_2.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsuNom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CarCodNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsuNom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CarCodNavigation.CarCod)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UsuCod">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UsuCod">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UsuCod">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
